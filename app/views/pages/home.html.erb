<h1>Cats!!</h1>
 <% @cats.each do |cat| %>
       <tr>
         <td><%= cat.name %></td>
       </tr>
     <% end %>
 
 <h2>Todos!</h2>
 <% @todos.each do |todo| %>
       <tr>
         <td><%= todo.jobs %></td>
         <td><%= todo.done %></td>
       </tr>
     <% end %>
 
 <h3>Users</h3>
 <% @users.each do |user| %>
       <tr>
         <td><%= user.username %></td>
         <td><%= user.email %></td>
         <td><%= user.age %></td>
       </tr>
     <% end %>

<br></br>
 <%= form_for @td do |f| %>
   <%= f.label :jobs %>:
   <%= f.text_field :jobs %><br />
 
   <%= f.label :done %>:
   <%= f.text_field :done %><br />  
   <%= f.submit %>
     
 <% end %>