<h1>Cats</h1>
<% @cats.each do |cat| %>
       <tr>
         <td><%= cat.name %></td>
       </tr>
     <% end %>
 
<h1>Todos</h1>
<% @todos.each do |todo| %>
       <tr>
         <td><%= todo.tasks %></td>
         <td><%= todo.finished %></td>
       </tr>
     <% end %>
 
<h1>Users</h1>
<% @users.each do |user| %>
       <tr>
         <td><%= user.username %></td>
         <td><%= user.email %></td>
         <td><%= user.age %></td>
       </tr>
     <% end %>

<br></br>
